<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mynificent.ent" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Ent</name>
    <description>
        Apps Description.
    </description>
    <author email="kosmozasranec954@gmail.com" href="http://cordova.io">
      Munificent Studio
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <icon density="ldpi" src="www/ldpi.png" />
        <icon density="mdpi" src="www/mdpi.png" />
        <icon density="hdpi" src="www/hdpi.png" />
        <icon density="xhdpi" src="www/xhdpi.png" />
        <icon density="xxhdpi" src="www/xxhdpi.png" />
        <icon density="xxxhdpi" src="www/xxxhdpi.png" />
        <allow-intent href="market:*" />
    </platform>
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
    <engine name="android" spec="^7.1.1" />
</widget>
